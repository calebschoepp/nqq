// num -> num
print(num(7)); // expect: 7
print(num(1.2)); // expect: 1.2
print(num(1312.12)); // expect: 1312.12
print(num(1.0)); // expect: 1
print(num(-3)); // expect: -3
print(num(-32.20)); // expect: -32.2
print(num(0)); // expect: 0

// string -> num
print(num("0")); // expect: 0
print(num("1")); // expect: 1
print(num("1.0")); // expect: 1
print(num("2.3")); // expect: 2.3
print(num("-1")); // expect: -1
print(num("-1.3")); // expect: -1.3
